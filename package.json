{
  "name": "json2toon",
  "version": "1.0.1",
  "description": "Convert JSON to TOON (Token-Oriented Object Notation) format with token counting and cost analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "json2toon": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "json",
    "toon",
    "converter",
    "json2toon",
    "json-to-toon",
    "token-oriented",
    "llm",
    "serialization",
    "optimization",
    "token-count",
    "ai",
    "cost-savings",
    "compression"
  ],
  "author": "Sumit Patel <sumit.lk.patel@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/slkpatel/json2toon.git"
  },
  "bugs": {
    "url": "https://github.com/slkpatel/json2toon/issues"
  },
  "homepage": "https://github.com/slkpatel/json2toon#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
